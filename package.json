{
    "name": "@crawl-lab/react-native-calendar",
    "version": "1.0.5",
    "description": "ðŸ“† A highly customizable React Native calendar library offering a user-friendly UI and smooth animations. ",
    "main": "lib/commonjs/index",
    "module": "lib/module/index",
    "types": "lib/typescript/src/index.d.ts",
    "react-native": "src/index",
    "source": "src/index",
    "files": [
        "src",
        "lib",
        "android",
        "ios",
        "cpp",
        "*.podspec",
        "!ios/build",
        "!android/build",
        "!android/gradle",
        "!android/gradlew",
        "!android/gradlew.bat",
        "!android/local.properties",
        "!**/__tests__",
        "!**/__fixtures__",
        "!**/__mocks__",
        "!**/.*"
    ],
    "scripts": {
        "example": "yarn workspace react-native-calendar-example",
        "test": "jest",
        "typecheck": "tsc --noEmit",
        "lint": "eslint \"**/*.{js,ts,tsx}\"",
        "clean": "del-cli lib",
        "prepare": "husky && bob build",
        "release": "release-it"
    },
    "keywords": [
        "react-native",
        "ios",
        "android",
        "calendar"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yunchanpark/react-native-calendar.git"
    },
    "author": "yunchanpark <yongp99@nate.com> (https://github.com/yunchanpark)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/yunchanpark/react-native-calendar/issues"
    },
    "homepage": "https://github.com/yunchanpark/react-native-calendar#readme",
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "dependencies": {
        "@shopify/flash-list": "^1.6.4",
        "dayjs": "^1.11.11",
        "lodash.debounce": "^4.0.8"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-numeric-separator": "^7.18.6",
        "@babel/plugin-proposal-private-methods": "^7.18.6",
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@babel/preset-env": "^7.24.7",
        "@babel/preset-react": "^7.24.7",
        "@babel/preset-typescript": "^7.24.7",
        "@commitlint/config-conventional": "^17.0.2",
        "@eslint/compat": "^1.1.0",
        "@evilmartians/lefthook": "^1.5.0",
        "@react-native/eslint-config": "^0.74.85",
        "@release-it/conventional-changelog": "^8.0.1",
        "@testing-library/dom": "^10.3.0",
        "@testing-library/jest-dom": "^6.4.6",
        "@testing-library/react": "^16.0.0",
        "@types/jest": "^29.5.5",
        "@types/lodash.debounce": "^4.0.9",
        "@types/react": "^18.2.44",
        "@types/react-dom": "^18.3.0",
        "commitlint": "^19.3.0",
        "del-cli": "^5.1.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.3",
        "husky": "^9.0.11",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jsdom": "^24.1.0",
        "lint-staged": "^15.2.7",
        "prettier": "^3.0.3",
        "react": "18.2.0",
        "react-dom": "18.3.1",
        "react-native": "0.74.2",
        "react-native-builder-bob": "^0.20.0",
        "react-native-gesture-handler": "^2.17.1",
        "react-native-reanimated": "3.10.1",
        "release-it": "^17.4.1",
        "ts-jest": "^29.1.5",
        "typescript": "^5.5.3"
    },
    "peerDependencies": {
        "react": "*",
        "react-native": "*",
        "react-native-gesture-handler": ">=2.17.1",
        "react-native-reanimated": ">=3.10.1"
    },
    "resolutions": {
        "@types/react": "^18.2.44"
    },
    "workspaces": [
        "example"
    ],
    "packageManager": "yarn@3.6.1",
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "react-native-builder-bob": {
        "source": "src",
        "output": "lib",
        "targets": [
            "commonjs",
            "module",
            [
                "typescript",
                {
                    "project": "tsconfig.build.json"
                }
            ]
        ]
    }
}
